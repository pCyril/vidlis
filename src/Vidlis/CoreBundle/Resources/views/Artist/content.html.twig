<div id="right_content">
    <div id="feeds">
        <div class="lettersRow">
            {% for letter in letters %}
                <a href="{{ url('_artistLetter', {'artist': letter} ) }}" class="btnLetter" onclick="loadBox('/artists/{{ letter }}', 'tabArtist');  return false;">{{ letter }}</a>
            {% endfor %}
        </div>
        <div class="feeds_content">
        {% for artist in artists %}
            <a class="boxArtist">
                <div class="videoTitle">{{ artist.name }}</div>
                <div class="clear"></div>
                <div class="images" style="height: {{ ((artist.getImagesPresentation()|length) / 2)|round * 150 }}px;">
                    {% for images in artist.getImagesPresentation() %}
                        <img src="{{ images }}" width="158px">
                    {% endfor %}
                </div>
                <div class="clear"></div>
            </a>
        {% endfor %}
            <div class="clear"></div>
        </div>
    </div>
</div>

<script>
    var container = document.querySelector('#feeds.feeds_content');
    var msnry = new Masonry( container, {
        itemSelector: '.boxArtist'
    });
</script>
